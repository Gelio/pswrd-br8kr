{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}\\index.js",
      "args": [
        "-v",
        "-f",
        "wordlist.txt"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program (more requests)",
      "program": "${workspaceRoot}\\index.js",
      "args": [
        "-f",
        "wordlist.txt",
        "--max-requests",
        "1000"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch (solution)",
      "program": "${workspaceRoot}\\index.js",
      "args": [
        "-v",
        "-f",
        "solution.txt"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858
    }
  ]
}
